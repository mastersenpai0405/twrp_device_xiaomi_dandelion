# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# Currently we dont have e2fsck compiled. So fs check would failed.
# mount point   	fstype	device							flags

system		/system		ext4 	ro	wait,avb=vbmeta_system,logical,first_stage_mount,avb_keys=/avb/q-gsi.avbpubkey:/avb/r-gsi.avbpubkey:/avb/s-gsi.avbpubkey,
vendor 		/vendor		ext4	ro	wait,avb,logical,first_stage_mount,
product		/product	ext4	ro	wait,avb,logical,first_stage_mount,

/recovery		emmc	/dev/block/platform/bootdevice/by-name/recovery		flags=display="Recovery";backup=1
/boot			emmc	/dev/block/platform/bootdevice/by-name/boot		flags=display="Boot"
/cache			ext4	/dev/block/platform/bootdevice/by-name/cache		flags=display="Cache"
/metadata		ext4	/dev/block/platform/bootdevice/by-name/md_udc		flags=display="Metadata";backup=1
/data			f2fs	/dev/block/platform/bootdevice/by-name/userdata		flags=fileencryption=aes-256-xts
/protect_f		emmc	/dev/block/platform/bootdevice/by-name/protect1		flags=backup=1
/protect_s		emmc	/dev/block/platform/bootdevice/by-name/protect2		flags=backup=1
/nvdata			emmc	/dev/block/platform/bootdevice/by-name/nvdata		flags=backup=1
/nvcfg			emmc	/dev/block/platform/bootdevice/by-name/nvcfg		flags=backup=1
/persist		ext4	/dev/block/platform/bootdevice/by-name/persist		flags=display="Persist";backup=1
/persistent		emmc	/dev/block/platform/bootdevice/by-name/frp		flags=backup=1
/nvram			emmc	/dev/block/platform/bootdevice/by-name/nvram		flags=backup;display="NVRAM"
/proinfo		emmc	/dev/block/platform/bootdevice/by-name/proinfo		flags=backup=1
/bootloader		emmc	/dev/block/platform/bootdevice/by-name/lk		flags=backup;display="Bootloader"
/bootloader2		emmc	/dev/block/platform/bootdevice/by-name/lk2		flags=backup=1
/dtbo			emmc	/dev/block/platform/bootdevice/by-name/dtbo		flags=backup=1
/vbmeta			emmc	/dev/block/platform/bootdevice/by-name/vbmeta		flags=display="VBMeta";backup=1;flashimg=1
/tee1			emmc	/dev/block/platform/bootdevice/by-name/tee1
/tee2			emmc	/dev/block/platform/bootdevice/by-name/tee2
/boot_para		emmc	/dev/block/platform/bootdevice/by-name/boot_para

# External Storage
/external_sd		auto	/dev/block/mmcblk1p1	/dev/block/mmcblk1		flags=display="MicroSD Card";storage;wipeingui;removable
/usb-otg		vfat	/dev/block/sda1		/dev/block/sdd			flags=storage;wipeingui;removable
